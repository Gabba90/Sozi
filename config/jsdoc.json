{
    "templates": {
        "cleverLinks": false,
        "navigation": [
            {
                "label": "Github",
                "href": "https://github.com/sozi-projects/Sozi.git"
            },
            {
                "label": "Home page",
                "href": "https://sozi.baierouge.fr"
            }
        ],
        "default": {
            "staticFiles": {
                "include": [
                    "doc/sozi-logo-bg.png"
                ]
            }
        }
    },
    "tags": {
        "allowUnknownTags": ["category"]
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "destination": "dist/api",
        "readme": "README.jsdoc.md"
    }
}
